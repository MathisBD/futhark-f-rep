__kernel void copy_18387(int64_t virt_num_groups_18392, int64_t num_groups_18393, __global unsigned char *mem_18131, __global unsigned char *mem_18160)
{
    const int block_dim0 = 0;
    const int block_dim1 = 1;
    const int block_dim2 = 2;
    int32_t copy_ltid_18388;
    int64_t group_sizze_18390;
    int32_t copy_gid_18389;
    
    copy_ltid_18388 = get_local_id(0);
    group_sizze_18390 = get_local_size(0);
    copy_gid_18389 = get_group_id(0);
    
    int32_t copy_gtid_18387 = copy_gid_18389 * group_sizze_18390 + copy_ltid_18388;
    int32_t phys_group_id_18394;
    
