__kernel void copy_18330(int64_t virt_num_groups_18335, int64_t num_groups_18336, __global unsigned char *mem_18128, __global unsigned char *mem_18151)
{
    const int block_dim0 = 0;
    const int block_dim1 = 1;
    const int block_dim2 = 2;
    int32_t copy_ltid_18331;
    int64_t group_sizze_18333;
    int32_t copy_gid_18332;
    
    copy_ltid_18331 = get_local_id(0);
    group_sizze_18333 = get_local_size(0);
    copy_gid_18332 = get_group_id(0);
    
    int32_t copy_gtid_18330 = copy_gid_18332 * group_sizze_18333 + copy_ltid_18331;
    int32_t phys_group_id_18337;
    
    phys_group_id_18337 = get_group_id(0);
    
    int32_t iterations_18338 = sdiv_up32(sext_i64_i32(virt_num_groups_18335) - phys_group_id_18337, sext_i64_i32(num_groups_18336));
    
    for (int32_t i_18339 = 0; i_18339 < iterations_18338; i_18339++) {
        int32_t virt_group_id_18340 = phys_group_id_18337 + i_18339 * sext_i64_i32(num_groups_18336);
        int64_t global_tid_18341 = sext_i32_i64(virt_group_id_18340) * sext_i32_i64(group_sizze_18333) + sext_i32_i64(copy_ltid_18331);
        int64_t slice_18344 = (int64_t) 14;
        int64_t slice_18345 = (int64_t) 14 * slice_18344;
        int64_t copy_i_18342 = squot64(global_tid_18341, slice_18344);
        int64_t remnant_18346 = global_tid_18341 - copy_i_18342 * slice_18344;
        int64_t copy_i_18343 = remnant_18346;
        int64_t remnant_18347 = remnant_18346 - copy_i_18343;
        
        if (slt64(global_tid_18341, (int64_t) 196)) {
            float tmp_18348 = ((__global float *) mem_18128)[copy_i_18343 * (int64_t) 14 + copy_i_18342];
            
            ((__global float *) mem_18151)[copy_i_18342 * (int64_t) 14 + copy_i_18343] = tmp_18348;
        }
        barrier(CLK_GLOBAL_MEM_FENCE | CLK_LOCAL_MEM_FENCE);
    }
    
  error_0:
    return;
}
